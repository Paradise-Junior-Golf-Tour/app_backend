{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "fee": {
      "type": "decimal",
      "required": true,
      "default": 10,
      "min": 0
    },
    "transactions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::transaction.transaction",
      "inversedBy": "events"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "date_start": {
      "type": "date",
      "required": true,
      "default": "2022-10-29"
    },
    "date_end": {
      "type": "date",
      "required": true,
      "default": "2022-10-31"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "events"
    },
    "result": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::result.result",
      "inversedBy": "event"
    },
    "announcement": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::announcement.announcement",
      "inversedBy": "events"
    },
    "max_users": {
      "type": "integer"
    },
    "date": {
      "type": "date"
    },
    "registration_start_date": {
      "type": "date"
    },
    "registration_end_date": {
      "type": "date"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "address_1": {
      "type": "string",
      "required": true
    },
    "address_2": {
      "type": "string",
      "required": false
    },
    "address_city": {
      "type": "string",
      "required": true
    },
    "address_state": {
      "type": "string",
      "required": true
    },
    "address_zip": {
      "type": "string",
      "required": true
    },
    "start": {
      "type": "time",
      "required": true
    }
  }
}
