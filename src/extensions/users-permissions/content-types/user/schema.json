{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "admin": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "users"
    },
    "tee_times": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tee-time.tee-time",
      "inversedBy": "users"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "name_first": {
      "type": "string",
      "required": true
    },
    "name_last": {
      "type": "string",
      "required": true
    },
    "age": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "birthday": {
      "type": "date",
      "required": true
    },
    "address_one": {
      "type": "string",
      "required": true
    },
    "address_two": {
      "type": "string"
    },
    "address_city": {
      "type": "string"
    },
    "address_state": {
      "type": "string"
    },
    "address_zip": {
      "type": "integer",
      "required": true
    },
    "member": {
      "type": "boolean",
      "default": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "required": true
    },
    "fundraisers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::fundraiser.fundraiser",
      "mappedBy": "users_permissions_users"
    },
    "results": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::result.result",
      "inversedBy": "users"
    },
    "tee_time_slots": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tee-time-slot.tee-time-slot",
      "inversedBy": "users_permissions_users"
    }
  }
}
